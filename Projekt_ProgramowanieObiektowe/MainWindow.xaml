<Window x:Class="Projekt_ProgramowanieObiektowe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt_ProgramowanieObiektowe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="300">
    <Grid>
        <Button x:Name="showStorage" Content="Show Storage" HorizontalAlignment="Center" VerticalAlignment="Top" Width="112" Height="44" Click="showStorage_Click" Margin="90,90,90,0"/>
        <Button x:Name="showProducts" Content="Show Products" HorizontalAlignment="Center" VerticalAlignment="Top" Width="112" Height="44" Click="ShowProducts_Click" Margin="90,180,90,0"/>
        <Button x:Name="manageReceipts" Content="Manage Receipts " HorizontalAlignment="Center" VerticalAlignment="Top" Width="112" Height="44" Click="manageReceipts_Click" Margin="90,270,90,0"/>
    </Grid>
</Window>
